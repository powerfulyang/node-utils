{
  "name": "@powerfulyang/node-utils",
  "version": "1.0.9",
  "description": "common utils",
  "main": "dist/index.js",
  "sideEffects": false,
  "author": {
    "name": "powerfulyang"
  },
  "repository": "https://github.com/powerfulyang/tools",
  "license": "MIT",
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "patch-version": "npm version patch --no-git-tag-version",
    "build": "rimraf dist && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "prettier": "prettier \"src/**/*.ts\" --write",
    "lint:js": "eslint --ext .ts ./src --fix",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "precommit": "npm run test && lint-staged && npm run tsc",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@powerfulyang/lint": "0.7.14",
    "@types/ip": "1.1.0",
    "@types/sharp": "0.30.0",
    "npm-run-all": "4.1.5",
    "sharp": "0.30.3"
  },
  "peerDependencies": {
    "sharp": "^0.30.2"
  },
  "dependencies": {
    "@powerfulyang/utils": "^1.3.10",
    "ip": "^1.1.5"
  }
}
