{
  "name": "@powerfulyang/node-utils",
  "version": "1.0.8",
  "description": "common utils",
  "main": "dist/index.js",
  "sideEffects": false,
  "author": {
    "name": "powerfulyang"
  },
  "repository": "https://github.com/powerfulyang/tools",
  "license": "MIT",
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "patch-version": "npm version patch --no-git-tag-version",
    "build": "rimraf dist && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "prettier": "prettier \"src/**/*.ts\" --write",
    "lint:js": "eslint --ext .ts ./src --fix",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "precommit": "npm run test && npm run lint:js && npm run prettier && npm run tsc",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@powerfulyang/lint": "^0.7.10",
    "@types/ip": "^1.1.0",
    "@types/sharp": "^0.29.4",
    "npm-run-all": "^4.1.5",
    "sharp": "^0.29.3"
  },
  "peerDependencies": {
    "sharp": "^0.29.3"
  },
  "dependencies": {
    "@powerfulyang/utils": "^1.3.6",
    "ip": "^1.1.5"
  }
}
